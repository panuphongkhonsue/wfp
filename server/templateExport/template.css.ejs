body {
    font-family: 'Roboto', sans-serif !important;
}
div{
    font-family: "Bai Jamjuree", serif !important;
    font-weight: 400 !important;
    font-style: normal !important;
}
.font-regular {
    font-family: "Bai Jamjuree", serif !important;
    font-weight: 400 !important;
    font-style: normal !important;
}
font-bold{
    font-family: "Bai Jamjuree", serif !important;
    font-weight: 700 !important;
    font-style: normal !important;
}
.font-20{
    font-size : 20px !important;
}
.font-16{
    font-size : 16px !important;
}
.font-14{
    font-size : 14px !important;
}
.page-break {
    page-break-inside: avoid !important; 
    page-break-before: always !important;
    break-before: page !important; 
}
@media print {
    .page-break {
        page-break-before: always;
    }
}
.content {
    margin : 16px !important;
}
.d-flex{
    display:flex;
}
.row {
    display: flex !important;
    flex-wrap: wrap !important;
    margin-left: -8px !important;
    margin-right: -8px !important;
}
.justify-end {
    justify-content: end;
}
.py-md{
    padding-top:16px;
    padding-bottom : 16px;
}
.pb-md{
    padding-bottom:16px;
}
.pt-md{
    padding-top:16px;
}
.pl-md{
    padding-left: 16px;
}
.pl-sm{
    padding-left: 8px;
}
.mx-sm{
    margin-left : 8px;
    margin-right : 8px;
}
.py-sm{
    padding-top:8px;
    padding-bottom : 8px;
}
.pb-sm{
    padding-bottom:8px;
}
.pt-sm{
    padding-top:8px;
}
.px-md{
    padding-left : 16px;
    padding-right : 16px;
}
.px-sm{
    padding-left : 8px;
    padding-right : 8px;
}
.px-none{
    padding-left : 0px;
    padding-right : 0px;
}
.justify-center {
    justify-content: center !important;
}
.col {
    flex: 1 1 0%;
    max-width: 100%;
}
.col-12 {
    flex: 0 0 100%;
    max-width: 100%;
}
.col-sign {
    flex: 0 0 100%;
    max-width: 100%;
    display: flex;
    justify-content: center;
}
.col-position{
    flex: 0 0 30%;
    max-width: 100%;
    display: flex;
    justify-content: space-evenly;
}
.title-container {
    flex: 1;
    text-align: center;
}
.text-white{
    color: rgba(255, 255, 255, 0);
}
table {
    font-family: "<%= fontName %>", sans-serif;
    border-collapse: collapse;
    width: 100%;
}
td, th {
    font-size : 14px;
    border: 1px solid black;
    text-align: left;
    padding: 8px 8px 8px 16px; 
}

.bor{
    border : 1px solid red;
}